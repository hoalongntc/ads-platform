{
  "name": "TrackingClick",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "indexes": {
    "key_index": {
      "keys": {
        "mac": 1,
        "advertiserId": 1,
        "campaignId": 1,
        "bannerId": 1,
        "locationId": 1
      }
    },
    "key_processed": {
      "keys": {
        "processed": 1
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "http": {
    "path": "/clicks"
  },
  "mixins": {
    "TimeStamp": {
      "validateUpsert": true,
      "required": false
    }
  },
  "properties": {
    "mac": {
      "type": "string"
    },
    "advertiserId": {
      "type": "string"
    },
    "advertiserName": {
      "type": "string"
    },
    "campaignId": {
      "type": "string"
    },
    "campaignName": {
      "type": "string"
    },
    "bannerId": {
      "type": "string"
    },
    "bannerName": {
      "type": "string"
    },
    "locationId": {
      "type": "string"
    },
    "locationName": {
      "type": "string"
    },
    "os": {
      "type": "string"
    },
    "device": {
      "type": "string"
    },
    "device_type": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "age": {
      "type": "string"
    },
    "income": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "favorite": {
      "type": "string"
    },
    "userType": {
      "type": "string"
    },
    "startTime": {
      "type": "date"
    },
    "stopTime": {
      "type": "date"
    },
    "sessionTime": {
      "type": "number"
    },
    "dataUsage": {
      "type": "number"
    },
    "processed": {
      "type": "string",
      "default": "not_yet"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
