{
  "name": "ReportTracking2",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "indexes": {
    "key_index": {
      "keys": {
        "advertiserId": 1,
        "campaignId": 1,
        "locationId": 1,
        "reportDate": -1
      },
      "options": {
        "unique": true
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "advertiserId": {
      "type": "string",
      "required": true
    },
    "advertiserName": {
      "type": "string"
    },
    "campaignId": {
      "type": "string",
      "required": true
    },
    "campaignName": {
      "type": "string"
    },
    "locationId": {
      "type": "string",
      "required": true
    },
    "locationName": {
      "type": "string"
    },
    "clickCount": {
      "type": "number",
      "default": 0
    },
    "impressionCount": {
      "type": "number",
      "default": 0
    },
    "reportDate": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
