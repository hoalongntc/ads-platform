{
  "name": "TrackingImpression",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "indexes": {
    "key_index": {
      "keys": {
        "mac": 1,
        "advertiserId": 1,
        "campaignId": 1,
        "bannerId": 1,
        "locationId": 1
      }
    },
    "key_processed": {
      "keys": {
        "processed": 1
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "http": {
    "path": "/impressions"
  },
  "mixins": {
    "TimeStamp": {
      "validateUpsert": true,
      "required": false
    }
  },
  "properties": {
    "mac": {
      "type": "string"
    },
    "advertiserId": {
      "type": "string"
    },
    "advertiserName": {
      "type": "string"
    },
    "campaignId": {
      "type": "string"
    },
    "campaignName": {
      "type": "string"
    },
    "bannerId": {
      "type": "string"
    },
    "bannerName": {
      "type": "string"
    },
    "locationId": {
      "type": "string"
    },
    "locationName": {
      "type": "string"
    },
    "count": {
      "type": "number",
      "default": 1
    },
    "processed": {
      "type": "string",
      "default": "not_yet"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
