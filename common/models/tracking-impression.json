{
  "name": "TrackingImpression",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "indexes": {
    "key_index": {
      "keys": {
        "mac": 1,
        "advertiserId": 1,
        "campaignId": 1,
        "bannerId": 1,
        "hotspotId": 1,
        "trackingDate": -1
      },
      "options": {
        "unique": true
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "http": {
    "path": "/impressions"
  },
  "properties": {
    "mac": {
      "type": "string",
      "required": true
    },
    "advertiserId": {
      "type": "string",
      "required": true
    },
    "advertiserName": {
      "type": "string"
    },
    "campaignId": {
      "type": "string",
      "required": true
    },
    "campaignName": {
      "type": "string"
    },
    "bannerId": {
      "type": "string",
      "required": true
    },
    "bannerName": {
      "type": "string"
    },
    "hotspotId": {
      "type": "string",
      "required": true
    },
    "hotspotName": {
      "type": "string"
    },
    "count": {
      "type": "number",
      "default": 1
    },
    "trackingDate": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
